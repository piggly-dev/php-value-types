{
    "name": "piggly/php-value-types",
    "description": "Immutable value objects to easy format and assert values.",
    "keywords": [
        "piggly",
        "orm",
        "data mapper",
        "value objects",
        "value types"
    ],
    "type": "library",
    "homepage": "https://github.com/piggly-dev/php-value-types",
    "autoload": {
        "psr-4": {
            "Piggly\\ValueTypes\\": "src/"
        }
    },
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "php": "^7.3 || ^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
